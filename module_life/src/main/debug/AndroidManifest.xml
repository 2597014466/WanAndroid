<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.senon.module_life">

    <uses-permission android:name="android.permission.INTERNET"/>


    <application
        android:name=".App_Life"
        android:allowBackup="true"
        android:icon="@mipmap/ic_logo"
        android:label="@string/life_app_name"
        android:roundIcon="@mipmap/life_ic_launcher_round"
        android:supportsRtl="true"
        tools:replace="android:icon,label"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".SplashActivity"></activity>
        <activity android:name=".activity.KnowledgeSystemActivity"></activity>

    </application>

</manifest>